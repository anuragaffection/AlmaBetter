{
    "manifest_version": 3,
    "name": "LinkedIn Extension",
    "version": "1.0",
    "description": "Custom extension for LinkedIn",
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "./images/icon.jpg",
            "48": "./images/icon.jpg",
            "128": "./images/icon.jpg"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "*://www.linkedin.com/*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "styles.css"
            ]
        }
    ],
    "icons": {
        "16": "./images/icon.jpg",
        "48": "./images/icon.jpg",
        "128": "./images/icon.jpg"
    }
}


